#!/usr/bin/env node

"use strict";

var Path = require("path");

var SpanishInquisition = require("../lib/spanish_inquisition");

try {
  SpanishInquisition.init(Path.resolve(__dirname, "../", "examples/primus.js"), function () {
    SpanishInquisition.server();
  }, function (err) {
    if (err) {
      console.error(err);
    }
  });
} catch (err) {
  console.error(err);
}
